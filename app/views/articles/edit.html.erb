<h1>Edit Article</h1>

<% if @article.errors.any? %>
 <h2>The following errors prevented the article from saving:</h2>
 <ul>
 <% @article.errors.full_messages.each do |msg|%>
 <li><%= msg %></li>
 <% end %>
 </ul>
<% end %>

<%= render 'form' %>

<button class="btn btn-primary"><%= link_to 'Back', articles_path %></button>

